{
	"source": {
		"include": [ "../Engine/Cameras", "../Engine/Core", "../Engine/Core/Resources", "../Engine/GameObjects", "../Engine/Lights", "../Engine/Particles", "../Engine/Physics", "../Engine/Renderables", "../Engine/Shaders", "../Engine/Material.js", "../Engine/Scene.js"],
		"exclude": [ /* array of paths to exclude */ ],
		"includePattern": ".+\\.js(doc)?$",
		"excludePattern": "(^|\\/|\\\\)_"
	},
    "plugins": [],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
	"opts": {
		"template": "templates/default",  // same as -t templates/default
		"encoding": "utf8",               // same as -e utf8
		"destination": "docs/",          // same as -d ./out/
		"recurse": true,                  // same as -r
		"readme": "README.me"
	}
}
